<div class="col-md-4">
    <div class="list-group">
        {{#each model as |item|}}
            {{#link-to 'demographic.choices.edit' item.id class='list-group-item list-group-item-action'}}
                <h5 class="list-group-item-heading">
                    {{item.name}}
                </h5>

                <p class="list-group-item-text">{{item.question.name}}</p>
            {{/link-to}}
        {{else}}
            <div class="list-group-item list-group-item-action">
                <p class="list-group-item-text">No results found.</p>
            </div>
        {{/each}}
    </div>
</div>
<div class="col-md-6">{{ outlet }}</div>
